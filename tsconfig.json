{
    "compilerOptions": {
        "target": "ES2020",
        "module": "CommonJS",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "outDir": "./bin/",
        "sourceMap": true,
        "noImplicitAny": false
    },
    "type": "module",
    "display": "Recommended",
    "$schema": "https://json.schemastore.org/tsconfig"
}